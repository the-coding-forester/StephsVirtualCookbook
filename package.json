{
  "name": "stephs-virtual-cookbook",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:unit  && npm run test:coverage ",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "typeorm": "typeorm-ts-node-commonjs",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@tsed/ajv": "^7.76.1",
    "@tsed/common": "^7.76.1",
    "@tsed/core": "^7.76.1",
    "@tsed/di": "^7.76.1",
    "@tsed/engines": "^7.76.1",
    "@tsed/exceptions": "^7.76.1",
    "@tsed/json-mapper": "^7.76.1",
    "@tsed/logger": "^6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/openspec": "^7.76.1",
    "@tsed/platform-cache": "^7.76.1",
    "@tsed/platform-exceptions": "^7.76.1",
    "@tsed/platform-express": "^7.76.1",
    "@tsed/platform-log-middleware": "^7.76.1",
    "@tsed/platform-middlewares": "^7.76.1",
    "@tsed/platform-params": "^7.76.1",
    "@tsed/platform-response-filter": "^7.76.1",
    "@tsed/platform-views": "^7.76.1",
    "@tsed/schema": "^7.76.1",
    "@tsed/swagger": "^7.76.1",
    "ajv": "^8.17.1",
    "barrelsby": "^2.8.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "method-override": "^3.0.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "5.2.0",
    "@tsed/cli-plugin-mocha": "5.2.0",
    "@tsed/cli-plugin-typeorm": "5.2.0",
    "@types/chai": "^4.3.16",
    "@types/chai-as-promised": "^7.1.8",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/method-override": "^0.0.35",
    "@types/mocha": "^10.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.11",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "mocha": "^10.7.0",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.3",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.3"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "npm",
    "runtime": "node"
  }
}
